{
    "name": "apexglobal/apex-starter-kit",
    "description": "Production-ready Laravel starter kit with a full admin panel, role-based access control, security logs, IP blocking, CSV/PDF exports, real-time charts, and a beautiful Tailwind UI â€” fully installed in one command.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "starter-kit",
        "admin-panel",
        "dashboard",
        "jetstream",
        "tailwind",
        "livewire",
        "roles",
        "permissions",
        "spatie",
        "security-logs",
        "ip-blocking",
        "rbac",
        "user-management",
        "export-csv",
        "export-pdf",
        "charts",
        "authentication",
        "admin",
        "apex"
    ],
    "authors": [
        {
            "name": "Apex Global Technologies",
            "email": "contact@apexglobal.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "livewire/livewire": "^3.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "ApexGlobal\\ApexStarterKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApexGlobal\\ApexStarterKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApexGlobal\\ApexStarterKit\\ApexStarterKitServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
